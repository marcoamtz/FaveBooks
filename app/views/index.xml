<Alloy>
	<Collection src="books"/>
	<Window class="container" fullscreen="false" title="My Books">
		<Menu id="menu" platform="android"></Menu>
		<ListView id="itemList">
			<ListSection dataCollection="books">
				<ListItem title="{title}" author="{author}"></ListItem>
			</ListSection>
		</ListView>
	</Window>
</Alloy>